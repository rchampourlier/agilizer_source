<h3>Assigned estimate</h3>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Team Member</th>
      <th>Dev</th>
      <th>Review</th>
    </tr>
  </thead>
  <tbody>
    <% statistics = presenter('issue_statistics', statistics) %>
    <% statistics.team_members.each do |team_member| %>
      <tr>
        <td><%= team_member %></td>
        <td><%= statistics.get(:time_estimate, :developer, team_member) %></td>
        <td><%= statistics.get(:time_estimate, :reviewer, team_member) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
