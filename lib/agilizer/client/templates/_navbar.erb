<div class="col-sm-3 col-md-2 sidebar">
  <ul class="nav nav-sidebar">
    <% navbar_items.each do |item| %>
      <% if request.path =~ %r{^/client/#{item[:link]}$} %>
        <li class="active"><a href="/client/<%= item[:link] %>"><%= item[:title] %> <span class="sr-only">(current)</span></a></li>
      <% else %>
        <li><a href="/client/<%= item[:link] %>"><%= item[:title] %></a></li>
      <% end %>
    <% end %>
  </ul>
